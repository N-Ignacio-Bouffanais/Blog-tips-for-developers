{
    "env": {
      "node": true,
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 4],
      "linebreak-style": ["error", "window"],
      "quotes": ["warn", "double"],
      "semi": ["warn", "never"]
    }
}
